{
  "name": "energi3-mycrypto",
  "version": "1.2.1",
  "vcs": "git",
  "rms": "aws",
  "rmsRepo": "s3://download.energi.software/internal/gen3/mycrypto",
  "tools": {
    "node": "8",
    "yarn": "*"
  },
  "entryPoints": {
    "web": {
      "tool": "nginx",
      "path": "dist/prod",
      "tune": {
        "socketPort": 8080,
        "socketType": "tcp"
      }
    }
  },
  "webcfg": {
    "root": "dist/prod",
    "mounts": {
      "/": {
        "static": true,
        "tune": {
          "etag": true,
          "pattern": true,
          "staticGzip": true,
          "spaRoutes": true
        }
      },
      "~ \\.(woff|woff2|eot|ttf)$": {
        "static": true,
        "tune": {
          "expires": "max",
          "pattern": true
        }
      },
      "~ \\.(js|css)$": {
        "static": true,
        "tune": {
          "expires": "max",
          "pattern": true,
          "staticGzip": true
        }
      },
      "/common/assets/": {
        "static": true,
        "tune": {
          "expires": "max",
          "pattern": true,
          "staticGzip": true
        }
      },
      "/node_modules": {
        "static": true,
        "tune": {
          "etag": "on",
          "pattern": true,
          "staticGzip": true
        }
      }
    }
  },
  "actions": {
    "build": [
      "@cte yarn build",
      "@cid tool build gzip"
    ]
  },
  "package": [
    "dist/prod"
  ]
}
